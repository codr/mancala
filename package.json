{
  "name": "Mancala",
  "version": "1.0.0",
  "description": "Simple mancala game using Redux with React and Babel support",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codr/mancala.git"
  },
  "scripts": {
    "build": "env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "start": "webpack-dev-server --progress --inline --port 8888 --host 0.0.0.0",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.js'",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint ."
  },
  "author": "Cody Coljee-Gray",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^2.10.0",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "firebase": "^2.4.2",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-modal": "^1.2.1",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4"
  }
}
